  box-sizing: border-box;
}
.container .content .van-tabs .van-tabs__wrap {
  height: 100%;
  width: 100%;
  box-sizing: border-box;
}
.container .content .van-tabs .van-tabs__wrap .van-tabs__scroll {
  height: 100%;
  width: 100%;
  margin: 0;
  box-sizing: border-box;
}
.container .content .van-tabs .van-tabs__wrap .van-tabs__scroll .van-tabs__nav {
  height: 100%;
  width: 100%;
  border: 0;
  box-sizing: border-box;
}
.container .content .van-tabs .van-tabs__wrap .van-tabs__scroll .van-tabs__nav .van-tab {
  height: 80rpx;
  line-height: 80rpx;
  background-color: #E7F1FE;
